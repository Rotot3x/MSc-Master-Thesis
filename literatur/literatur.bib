  keywords = {Niedrig},
  file = {C:\Users\Ferris Menzel\Zotero\storage\CR4U8MYV\Mustafa et al. - 2025 - Cross-Layer Analysis of Machine Learning Models for Secure and Energy-Efficient IoT Networks.pdf}
