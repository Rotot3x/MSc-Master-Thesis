@article{radanliev_ReviewComparisonUSEUUKRegulationsCyberRiskSecurityCurrentBlockchainTechnologies_2023,
  title = {Review and {{Comparison}} of {{US}}, {{EU}}, and {{UK Regulations}} on {{Cyber Risk}}/{{Security}} of the {{Current Blockchain Technologies}}: {{Viewpoint}} from 2023},
  shorttitle = {Review and {{Comparison}} of {{US}}, {{EU}}, and {{UK Regulations}} on {{Cyber Risk}}/{{Security}} of the {{Current Blockchain Technologies}}},
